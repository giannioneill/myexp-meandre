<% for channelmessage in @channelmessages %><% next if channelmessage.sender_id == @user.id %>new Insertion.Bottom('stage', '<%= escape_javascript channelmessage_content(channelmessage) %>');<% end %>
<% if @channelmessages.length > 0 %>$('conversation').scrollTop = $('conversation').scrollHeight + $('conversation').clientHeight;<% end %>