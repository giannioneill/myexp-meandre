<h1>New moderatorship</h1>

<%= error_messages_for :moderatorship %>

<% form_for(:moderatorship, :url => moderatorships_path(@forum)) do |f| %>
  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', moderatorships_path(@forum) %>